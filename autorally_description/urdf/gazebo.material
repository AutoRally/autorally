material Gazebo/TurquoiseGlowOutline
{
  technique
  {
    pass ambient
    {
      scene_blend alpha_blend
      //lighting off

      diffuse 0 1 1 1
      specular .1 .1 .1 128
    }

    pass ambient2
    {
      scene_blend alpha_blend

      diffuse 0 1 1
      specular .1 .1 .1 128
      emissive 0 1 1

      polygon_mode wireframe
    }
  }
}

material Gazebo/RedTransparentOverlay
{
  technique
  {
    pass
    {
      scene_blend alpha_blend
      depth_write off
      lighting off
      depth_check off

