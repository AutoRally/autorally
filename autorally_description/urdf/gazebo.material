material Gazebo/Texture_blend
{
  technique
  {
    pass
    {
      ambient 0.1 0.1 0.1 0.1
      diffuse 0.8 0.8 0.8 1.0
      specular 0.01 0.01 0.01 1.0 2.0

      texture_unit
      {
        texture ground_dirt_texture.jpg
        filtering trilinear
        scale .01 .01
      }
      texture_unit
      {
        texture blended_texture.png
        colour_op alpha_blend
      }
      texture_unit
      {
        texture ground_grass_texture.jpg
        colour_op_ex blend_current_alpha src_texture src_current
        alpha_op_ex source1 src_manual src_current 1.0
        scale .01 .01
      }
    }
  }
}

